{
  "name": "@epcc-sdk/sdks-shopper",
  "version": "0.0.42",
  "description": "",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./react-query": {
      "import": {
        "types": "./dist/react-query.d.ts",
        "default": "./dist/react-query.mjs"
      },
      "require": {
        "types": "./dist/react-query.d.cts",
        "default": "./dist/react-query.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm oas:build && pnpm oas:openapi-ts && pnpm build:tsup",
    "build:tsup": "tsup",
    "oas:build": "pnpm oas:redocly:bundle && pnpm oas:openapi-format:convert && pnpm oas:redocly:join:shopper",
    "oas:openapi-ts": "openapi-ts",
    "oas:redocly": "redocly --config ./redocly.json",
    "oas:redocly:lint": "redocly lint --config ../specs/config/redocly.yaml",
    "oas:redocly:join:shopper": "redocly join --config ../specs/config/redocly.yaml --output ../specs/shopper.yaml ../specs/bundled/catalog_view_3.1.yaml ../specs/bundled/cart_checkout.yaml ../specs/bundled/subscriptions_3.1.yaml ../specs/bundled/inventories_3.1.yaml ../specs/bundled/authentication.yaml ../specs/bundled/account-addresses.yaml ../specs/bundled/account_management.yaml ../specs/bundled/authentication-realms.yaml ../specs/bundled/currencies.yaml  ../specs/bundled/files.yaml ../specs/bundled/catalog_search_3.1.yaml",
    "oas:redocly:bundle": "redocly bundle --config ../specs/config/redocly.yaml",
    "oas:openapi-format:convert": "pnpm dlx openapi-format ../specs/bundled/catalog_view.yaml -o ../specs/bundled/catalog_view_3.1.yaml --convertTo \"3.1\" && pnpm dlx openapi-format ../specs/bundled/subscriptions.yaml -o ../specs/bundled/subscriptions_3.1.yaml --convertTo  \"3.1\" && pnpm dlx openapi-format ../specs/bundled/inventories.yaml -o ../specs/bundled/inventories_3.1.yaml --convertTo  \"3.1\" && pnpm dlx openapi-format ../specs/bundled/catalog_search.yaml -o ../specs/bundled/catalog_search_3.1.yaml --convertTo \"3.1\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist/**"
  ],
  "keywords": [],
  "devDependencies": {
    "@hey-api/openapi-ts": "0.61.1",
    "@redocly/cli": "^1.21.0",
    "@redocly/openapi-core": "^1.21.0",
    "@tanstack/react-query": "^5.69.0",
    "@testing-library/jest-dom": "6.6.3",
    "@types/ejs": "^3.1.5",
    "ejs": "^3.1.10",
    "esbuild-fix-imports-plugin": "^1.0.19",
    "esbuild-plugin-file-path-extensions": "^1.0.0",
    "jsdom": "26.1.0",
    "lodash": "^4.17.21",
    "tsup": "^8.4.0",
    "typescript": "^5.5.3",
    "vitest": "3.1.4"
  },
  "dependencies": {
    "@hey-api/client-fetch": "0.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.69.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": {
      "optional": true
    }
  }
}
