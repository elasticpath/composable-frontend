{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**", "dist-cjs/**", "dist-es/**", "dist-types/**", "build/**"]
    },
    "@epcc-sdk/sdks-catalog-search#build": {
      "dependsOn": ["@epcc-sdk/sdks-shopper#build"],
      "outputs": ["dist/**"]
    },
    "generate": {
      "outputs": []
    },
    "examples": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": []
    },
    "test:watch": {
      "cache": false
    },
    "test:e2e": {
      "outputs": []
    },
    "build:e2e": {
      "outputs": []
    },
    "start:e2e": {
      "outputs": []
    },
    "start": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build"]
    }
  }
}
